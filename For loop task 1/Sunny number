package problemsloving;

public class Sunnynum {
	public static void main(String args[])
	{
		int n=8;
		boolean isSunny= false;
		for(int i=0;i<=n;i++)
		{
			if(i*i==n+1)
			{
			isSunny=true;	
			}
		}
		if(isSunny)
		{
			System.out.println("It is a sunny number");
		}
		else
		{
			System.out.println("It is Not a sunny number");

		}
	}

}
