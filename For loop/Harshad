package problemsloving;

public class Harshad {
	public static void main(String args[])
	{
		int n=18;
		int temp=n;
		int sum=0;
		while(n>0)
		{
			int dig=n%10;
			  
				sum+=dig;
				n/=10;
		}
		if(temp%sum==0)
		{
			System.out.println("Harshad");
		}
		else
		{
			System.out.println(" Not Harshad");
		}
	}

}
