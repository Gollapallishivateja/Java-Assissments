package problemsloving;

public class Strong {
	public static void main(String args[])
	{
	 
	int n=145;
	int temp=n;
	int sum=0;
	while(n>0)
	{
		int fact=1;
		int dig=n%10;
		for(int i=1;i<=dig;i++)
		{
			fact*=i;
		}
		n/=10;
		sum+=fact;
		
	}
if(sum==temp)
{
	System.out.println(sum+" is a Strong number.");
}
else
{
	System.out.println(sum+" is a not Strong number.");
}

}
}
